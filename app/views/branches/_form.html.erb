<%= simple_form_for(@branch) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :email %>
    <%= f.input :tel %>
    <%= f.input :city %>
    <%= f.input :blat %>
    <%= f.input :blng %>
    <%= f.association :user %>
    <%= f.association :transportation_company %>
    <%= f.input :cover %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
