<%= simple_form_for(@vehicle_model) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :seats_number %>
    <%= f.input :default_image %>
    <%= f.association :vehicle_brand %>
    <%= f.input :up_view_image %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
